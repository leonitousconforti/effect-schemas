{
    "name": "effect-schemas",
    "version": "0.0.11",
    "description": "GNU ustar tar implementation written using effect-ts",
    "keywords": [
        "effect-ts",
        "gnu",
        "tar"
    ],
    "homepage": "https://github.com/leonitousconforti/effect-schemas",
    "bugs": {
        "url": "https://github.com/leonitousconforti/effect-schemas/issues",
        "email": "leo@leoconforti.us"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leonitousconforti/effect-schemas.git"
    },
    "files": [
        "src/**/*.ts",
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map"
    ],
    "type": "module",
    "sideEffects": [],
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null,
        "./*/index": null
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            "./package.json": "./package.json",
            ".": "./dist/index.js",
            "./*": "./dist/*.js",
            "./internal/*": null,
            "./*/index": null
        },
        "provenance": false
    },
    "scripts": {
        "prepare": "effect-language-service patch",
        "clean": "rimraf *.tsbuildinfo docs dist coverage",
        "circular": "madge --circular src/*.ts",
        "codegen": "build-utils prepare-v4",
        "check": "tsc -b tsconfig.json",
        "lint": "oxlint && oxfmt --check",
        "lint-fix": "oxlint --fix && oxfmt --write",
        "build": "tsc -b tsconfig.json && pnpm babel",
        "babel": "babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
        "test": "vitest",
        "coverage": "vitest --coverage"
    },
    "devDependencies": {
        "@babel/cli": "7.28.6",
        "@babel/core": "7.29.0",
        "@babel/plugin-transform-export-namespace-from": "7.27.1",
        "@babel/plugin-transform-modules-commonjs": "7.28.6",
        "@changesets/cli": "2.29.8",
        "@changesets/config": "3.1.2",
        "@effect/build-utils": "0.8.9",
        "@effect/docgen": "https://pkg.pr.new/Effect-TS/docgen/@effect/docgen@e7fe055",
        "@effect/language-service": "0.73.0",
        "@effect/vitest": "0.27.0",
        "@types/node": "25.2.1",
        "@vitest/coverage-v8": "3.2.4",
        "babel-plugin-annotate-pure-calls": "0.5.0",
        "changesets-gitlab": "0.13.4",
        "effect": "3.19.16",
        "jscodeshift": "17.3.0",
        "madge": "8.0.0",
        "oxfmt": "0.28.0",
        "oxlint": "1.43.0",
        "prettier": "3.8.1",
        "prettier-plugin-jsdoc": "1.8.0",
        "rimraf": "6.1.2",
        "tsx": "4.21.0",
        "typescript": "5.9.3",
        "vite": "7.3.1",
        "vite-tsconfig-paths": "6.1.0",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "effect": "3.19.16"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "packageManager": "pnpm@9.15.9"
}
